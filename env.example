# 版本比较工具环境变量配置
# GitLab基础配置
GITLAB_URL=https://gitlab.example.com
GITLAB_TOKEN=your_gitlab_token_here

# 其他配置
TASK_URL_PREFIX=https://your-task-system.com/task/
DEBUG=false
LOG_LEVEL=INFO

# OpenAI配置（可选，用于AI分析）
OPENAI_API_KEY=your_openai_api_key_here

# 服务配置
PORT=9112

# 钉钉机器人配置（可选）
DINGTALK_WEBHOOK_URL=your_dingtalk_webhook_url_here
DINGTALK_SECRET=your_dingtalk_secret_here 