# GitLab配置
GITLAB_URL=https://gitlab.example.com

# 项目配置 - 支持多个项目
# BI Server项目 (原有的)
GITLAB_TOKEN_BI_SERVER=your_bi_server_token_here
GITLAB_PROJECT_ID_BI_SERVER=130

# 其他项目配置
GITLAB_TOKEN_GUANDATA_WEB=your_guandata_web_token_here
GITLAB_PROJECT_ID_GUANDATA_WEB=your_project_id_here

GITLAB_TOKEN_COMPLEX_REPORT_PRO=your_complex_report_pro_token_here
GITLAB_PROJECT_ID_COMPLEX_REPORT_PRO=your_project_id_here

GITLAB_TOKEN_DATA_MIND=your_data_mind_token_here
GITLAB_PROJECT_ID_DATA_MIND=your_project_id_here

GITLAB_TOKEN_DATA_SYNAPSE=your_data_synapse_token_here
GITLAB_PROJECT_ID_DATA_SYNAPSE=your_project_id_here

# 兼容性配置 (向后兼容)
GITLAB_TOKEN=your_gitlab_token_here
GITLAB_PROJECT_ID=your_project_id_here

# OpenAI配置（可选，用于AI分析）
OPENAI_API_KEY=your_openai_api_key_here

# 服务配置
PORT=5000
DEBUG=false

# 任务链接配置
TASK_URL_PREFIX=https://your-task-system.com/browse/

# 钉钉机器人配置（可选）
DINGTALK_WEBHOOK_URL=your_dingtalk_webhook_url_here
DINGTALK_SECRET=your_dingtalk_secret_here 